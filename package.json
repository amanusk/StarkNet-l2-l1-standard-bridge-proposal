{
  "name": "starknet-l2-l1-bridge-proposal",
  "version": "0.0.1",
  "description": "Proposal for bridging StarkNet tokens to L1",
  "main": "index.js",
  "scripts": {
    "compile:l1": "npx hardhat compile",
    "compile:l2": "npx hardhat starknet-compile --disable-hint-validation --cairo-path ./contracts/l2-contracts",
    "compile": "npm run compile:l1 && npm run compile:l2",
    "test:l1": "npx hardhat test ./test/l1-tests/*",
    "test:l2": "npx hardhat test --starknet-network integrated-devnet ./test/l2-tests/*",
    "test": "npm run test:l1 && npm run test:l2",
    "clean": "rm -rf ./artifacts ./cache ./typechain ./starknet-artifacts"
  },
  "author": "amanusk@starkware.co",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.8.0",
    "@shardlabs/starknet-hardhat-plugin": "^0.7.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.4",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.8.1",
    "prettier-plugin-solidity": "^1.1.0",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "starknet": "^4.17.1",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4",
    "yarn-upgrade-all": "^0.7.1"
  },
  "dependencies": {}
}
